{
  "name": "dea_bank_db",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "dev": "npx nodemon",
    "init_DEA_BANK": "set DB_NAME=DEA_BANK&& npx tsx ./src/db_init.ts",
    "init_Dep_1": "set DB_NAME=Department1&& npx tsx ./src/db_init.ts",
    "init_Dep_2": "set DB_NAME=Department2&& npx tsx ./src/db_init.ts",
    "init": "npm run init_DEA_BANK && npm run init_Dep_1 && npm run init_Dep_2",
    "build": "rimraf ./build && npx tsc",
    "start": "npm run build && node build/index",
    "lint": "npx eslint ./src",
    "format": "npx eslint ./src --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "adminjs": "^7.8.1",
    "axios": "^1.6.8",
    "express": "^4.19.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.2",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.4",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
